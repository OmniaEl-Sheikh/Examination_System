@model Branch
@{
    ViewData["Title"] = "createBranch";
    SelectList deptlst = new SelectList(ViewBag.deptList, "Instructor.InstructorId", "Name");
}


<h2 class="text-center mt-4 mb-4">Add New Branch</h2>
<div class="row addQuestion">
    <div class="col-md-6 offset-md-3">
    <form method="post">

            <div class="form-group mb-3">
                <label asp-for="BranchName" class="control-label"></label>
        <input asp-for="BranchName" class="form-control" />
      </div>

           
            <div class="form-group mb-3">
        <input type="Submit" value="Create" class="btn btn-primary" />
        </div>
    </form>

</div>
</div>

<div class="text-center mt-3">
    <a asp-action="ShowBranches" class="btn btn-info">Back to List</a>
</div>
