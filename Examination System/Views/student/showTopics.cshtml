@model IEnumerable<Topic>
@{
    ViewData["Title"] = "Topics";
}


    <div class="container mt-5 topic">
        <h1 class="text-center mb-4">@ViewData["Title"]</h1>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col"> Topic Names</th>
                  
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.TopicName</td>
                      
                    </tr>
                }
            </tbody>
        </table>
    </div>
